<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd http://www.springframework.org/schema/context https://www.springframework.org/schema/context/spring-context.xsd">
    <context:annotation-config/>
    <bean class="autowiring.usingAnnotation.Subject" name="java">
        <property name="subName" value="Java"/>
        <property name="subCode" value="103"/>
    </bean>

    <bean class="autowiring.usingAnnotation.Teacher" name="teacher">
        <property name="teacherName" value="Babulal Ganpatlal Aapte"/>
    </bean>
</beans>