<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd
    ">
    <bean class="autowiring.usingXml.Subject" name="subject">
        <property name="subName" value="Java"/>
        <property name="subCode" value="103"/>
    </bean>

    <bean class="autowiring.usingXml.Teacher" name="teacher" autowire="byName">
        <property name="teacherName" value="Babulal Ganpatlal Aapte"/>
    </bean>
</beans>